@{
    ViewData["Title"] = "Home Page";
}
@model IEnumerable<Received>
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width"/>
    <title>Index</title>
</head>
<body>
<h4>Customers</h4>
<hr/>
<table class="table table-bordered"  cellpadding="0" cellspacing="0">
    <tr>
        <th>Order Number</th>
        <th>Cust Name</th>
        <th>Model</th>
        <th>Type</th>
        <th>Qty</th>
    </tr>
    @foreach (Received customer in Model)
    {
        <tr>
            <td>@customer.OrderNumber</td>
            <td>@customer.CustomerName</td>
            <td>@customer.Product_Line_Desc</td>
            <td>@customer.TypeDeviceForSevice</td>
            <td>@customer.ReceivedQty</td>
        </tr>
    }
</table>
<div>
    <ul>
            <li> Total @ViewBag.Customers</li>
        <li>@ViewBag.Message</li>
    </ul>
</div>
 
</body>
</html>
